{
  "title": "Trending Products Dataset",
  "type": "object",
  "schemaVersion": 1,
  "actorOutputSchemaVersion": 1,
  "properties": {
    "rank": {
      "title": "Product Ranking",
      "type": "string",
      "description": "Global ranking position of the product (always starts from 1)",
      "template": "{{rank}}"
    },
    "product_id": {
      "title": "Product ID",
      "type": "string",
      "description": "Unique TikTok Shop product identifier",
      "template": "{{product_id}}"
    },
    "name": {
      "title": "Product Name",
      "type": "string",
      "description": "Full product name",
      "template": "{{name}}"
    },
    "price_display": {
      "title": "Price Display",
      "type": "string",
      "description": "Formatted price string (e.g., '$39.00')",
      "template": "{{price_display}}"
    },
    "units_sold": {
      "title": "Units Sold",
      "type": "string",
      "description": "Total number of units sold",
      "template": "{{units_sold}}"
    },
    "gmv": {
      "title": "Gross Merchandise Value",
      "type": "string",
      "description": "Total revenue generated by the product",
      "template": "{{gmv}}"
    },
    "creator_count": {
      "title": "Creator Count",
      "type": "string",
      "description": "Number of creators who featured this product",
      "template": "{{creator_count}}"
    },
    "product_img_url": {
      "title": "Product Image URL",
      "type": "string",
      "description": "High-quality product image URL",
      "template": "{{product_img_url}}"
    },
    "product_url": {
      "title": "Product URL",
      "type": "string",
      "description": "Direct TikTok Shop product page link",
      "template": "{{product_url}}"
    },
    "categories": {
      "title": "Categories",
      "type": "string",
      "description": "Array of product category tags",
      "template": "{{categories}}"
    },
    "region": {
      "title": "Region",
      "type": "string",
      "description": "Geographic region (us/uk)",
      "template": "{{region}}"
    },
    "days_period": {
      "title": "Days Period",
      "type": "string",
      "description": "Time period analyzed in days",
      "template": "{{days_period}}"
    },
    "scraped_at": {
      "title": "Scraped At",
      "type": "string",
      "description": "ISO timestamp when data was scraped",
      "template": "{{scraped_at}}"
    },
    "engagement_rate": {
      "title": "Engagement Rate",
      "type": "string",
      "description": "Calculated engagement metric (creators/videos)",
      "template": "{{engagement_rate}}"
    },
    "data_limitations": {
      "title": "Data Limitations",
      "type": "string",
      "description": "Information about data availability and limitations",
      "template": "{{data_limitations}}"
    }
  },
  "required": [
    "rank",
    "product_id",
    "name",
    "price_display",
    "units_sold",
    "gmv",
    "creator_count",
    "product_img_url",
    "product_url",
    "categories",
    "region",
    "days_period",
    "scraped_at",
    "engagement_rate",
    "data_limitations"
  ]
}